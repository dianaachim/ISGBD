<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DataView">
    <children>
        <TableView fx:id="tableView" layoutX="8.0" layoutY="80.0" prefHeight="200.0" prefWidth="260.0">
            <columns>
                <TableColumn fx:id="keyC" prefWidth="94.0" text="Key" />
                <TableColumn fx:id="valueC" prefWidth="165.0" text="Value" />
            </columns>
        </TableView>
        <Button fx:id="InsertBtn" layoutX="387.0" layoutY="73.0" mnemonicParsing="false" onAction="#Insert" text="NEW" textFill="#0e752a">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
        <Button fx:id="DelBtn" layoutX="495.0" layoutY="280.0" mnemonicParsing="false" onAction="#delete" text="DELETE" textFill="#f20c0c">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Label fx:id="label" layoutX="322.0" layoutY="163.0" text="Label" />
      <TextField fx:id="textField" layoutX="437.0" layoutY="159.0" />
      <Button fx:id="nextBtn" layoutX="300.0" layoutY="243.0" mnemonicParsing="false" onAction="#Next" text="Next and Insert" />
      <Label layoutX="278.0" layoutY="287.0" text="Key" />
      <TextField fx:id="deleteField" layoutX="322.0" layoutY="283.0" />
      <Button fx:id="updBtn" layoutX="469.0" layoutY="243.0" mnemonicParsing="false" onAction="#update" text="Next and Update" />
      <Button layoutX="108.0" layoutY="310.0" mnemonicParsing="false" onAction="#loaddata1" text="Refresh" />
    </children>
</AnchorPane>
